{
  "name": "xbox-social-intake-form",
  "description": "Social Intake App",
  "version": "1.0.3",
  "private": true,
  "author": "aDigital",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "shopify",
    "e-commerce"
  ],
  "scripts": {
    "analyze": "GATSBY_ENABLE_BUNDLE_ANALYZER=true npm run build",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "deploy": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id $AWS_CLOUDFRONT_DISTRIBUTION --paths \"/*\"",
    "start": "gatsby develop --host 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "lint:fix": "eslint --fix --ignore-path .gitignore \"src/**/*.+(js)\"",
    "format": "npm run prettier --write",
    "lint": "eslint src",
    "postinstall": "husky install",
    "cz": "git add . && cz",
    "release": "standard-version",
    "validate": "npm run lint && npm run prettier --list-different",
    "prepare": "husky install",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "build:redirects": "node ./scripts/buildRedirects.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@contentful/rich-text-types": "^16.0.2",
    "@headlessui/react": "1.7.2",
    "@headlessui/tailwindcss": "^0.1.2",
    "@heroicons/react": "^2.0.16",
    "@stitches/react": "1.2.8",
    "@studio-freight/lenis": "0.2.6",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tanstack/react-query": "^4.22.0",
    "atob": "^2.1.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "base64-string": "1.1.2",
    "clsx": "1.2.1",
    "eslint-config-gatsby-standard": "3.0.2",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.4.2",
    "gatsby": "4.23.0",
    "gatsby-plugin-facebook-pixel": "^1.0.8",
    "gatsby-plugin-google-gtag": "4.2.0",
    "gatsby-plugin-image": "2.23.0",
    "gatsby-plugin-manifest": "4.23.0",
    "gatsby-plugin-react-helmet": "5.23.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "4.23.0",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-transition-link": "1.20.5",
    "gatsby-source-contentful": "7.22.0",
    "gatsby-source-filesystem": "4.23.0",
    "gatsby-source-shopify": "7.12.0",
    "gatsby-source-shopify-translations": "^0.4.1",
    "gatsby-transformer-json": "^4.25.0",
    "gatsby-transformer-sharp": "4.23.0",
    "gsap": "3.11.1",
    "i18next": "^22.4.8",
    "jsdom": "20.0.2",
    "jsonp": "^0.2.1",
    "react": "18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "18.2.0",
    "react-fast-marquee": "1.3.5",
    "react-feather": "2.0.10",
    "react-flip-move": "3.0.4",
    "react-flip-toolkit": "7.0.16",
    "react-helmet": "6.1.0",
    "react-hook-form": "^7.43.8",
    "react-hot-toast": "2.4.0",
    "react-i18next": "^12.1.1",
    "react-slick": "^0.29.0",
    "react-use": "17.4.0",
    "shopify-buy": "2.16.1",
    "styled-components": "^5.3.6",
    "to-querystring": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@testing-library/dom": "8.18.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "autoprefixer": "10.4.12",
    "babel-preset-gatsby": "2.23.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.0.2",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "gatsby-plugin-postcss": "5.23.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "^13.1.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
